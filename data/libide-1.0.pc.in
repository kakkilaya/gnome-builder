prefix=@prefix@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${exec_prefix}/include

Name: LibIDE
Description: LibIDE contains the components used to build the GNOME Builder IDE.
Version: @VERSION@
Libs: -L${libdir}/gnome-builder -lide-1.0 -legg-private -Ltemplate-glib-1.0 -lpanel-gtk
Cflags: -I${includedir}/gnome-builder-@VERSION@/libide -I${includedir}/gnome-builder-@VERSION@/egg -I${includedir}/gnome-builder-@VERSION@/template-glib -I${includedir}gnome-builder-@VERSION@/pnl
Requires: gio-2.0 gtk+-3.0 gtksourceview-3.0
